<launch>
  <arg name="model" default="robot_event_cam"/>
  <arg name="multi_robot_name" default=""/>

  <group if="$(eval arg('model') == 'robot_2lidar')">
    <include file="$(find robo_base_bringup)/launch/merge_laser.launch"/>
  </group>

  <group if="$(eval arg('model') == 'tko_robot')">
    <include file="$(find robo_base_bringup)/launch/merge_laser_tko.launch"/>
  </group>



  <include file="$(find robo_base_bringup)/launch/includes/description.launch.xml">
    <arg name="model" value="$(arg model)" />
  </include>


  <node pkg="robot_state_publisher" type="robot_state_publisher" name="robot_state_publisher">
    <param name="publish_frequency" type="double" value="50.0" />
    <param name="tf_prefix" value="$(arg multi_robot_name)"/>
  </node>
</launch>